{
  "dashboard": {
    "id": null,
    "uid": "gx10-monitor",
    "title": "GX10 System Monitor",
    "description": "Comprehensive monitoring dashboard for GX10 AI workstation including CPU, Memory, GPU, and Brain status",
    "tags": ["gx10", "ai", "gpu", "system", "nvidia"],
    "timezone": "browser",
    "schemaVersion": 39,
    "version": 1,
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
